{
  "plugins": ["prettier"],
  "extends": [
    "next",
    "next/core-web-vitals",
    "prettier",
    "plugin:prettier/recommended"
  ],
  "settings": {
    "import/extensions": [".ts", ".tsx"]
  },
  "ignorePatterns": ["node_modules/"],
  "rules": {
    "@next/next/no-html-link-for-pages": "warn"
  }
}
